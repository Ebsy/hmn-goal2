{
  "name": "hmn-goal2",
  "version": "1.1.1",
  "description": "REST backend es6/babel/mongoose/jwt",
  "author": "Neil Ebrey <neil.ebrey@gmail.com>",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=4.8.0",
    "npm": ">=2.15.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ebsy/hmn-goal2.git"
  },
  "scripts": {
    "start": "gulp serve",
    "start:debug": "cross-env DEBUG=hmn-goal2:* npm start",
    "docs": "documentation build server/** -f html -o docs",
    "build": "gulp",
    "lint": "esw *.js server config --color",
    "lint:watch": "npm run lint -- --watch",
    "release": "standard-version",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout 10000 --ui bdd --reporter spec --colors --require babel-core/register server/tests --recursive --exit",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "1.4.3",
    "cors": "^2.8.4",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-validation": "^1.0.2",
    "express-winston": "^3.0.1",
    "helmet": "^3.14.0",
    "http-status": "^1.2.0",
    "joi": "^14.0.0",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.2.8",
    "morgan": "^1.9.1",
    "standard-version": "^4.4.0",
