    "chai": "^4.2.0",
    "commitizen": "^3.0.3",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "documentation": "^8.1.2",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-watch": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.4.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.7",
    "husky": "^1.1.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.1.0",
    "prettier": "^1.14.3",
    "run-sequence": "^2.2.1",
    "supertest": "^3.3.0"
